{
   "author": [ "Kento Nishi" ],
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon48.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "player-backward": {
         "description": "Rewind backward 10%",
         "global": true
      },
      "player-forward": {
         "description": "Rewind forward 10%",
         "global": true
      },
      "player-mute": {
         "description": "Mute",
         "global": false
      },
      "player-next": {
         "description": "Next Track",
         "global": true,
         "suggested_key": {
            "default": "MediaNextTrack"
         }
      },
      "player-previous": {
         "description": "Previous Track",
         "global": true,
         "suggested_key": {
            "default": "MediaPrevTrack"
         }
      },
      "player-repeat": {
         "description": "Toggle Repeat",
         "global": false
      },
      "player-save": {
         "description": "Toggle Favorite",
         "global": true,
         "suggested_key": {
            "default": "Ctrl + Shift + Space"
         }
      },
      "player-shuffle": {
         "description": "Toggle Shuffle",
         "global": false
      },
      "player-toggle": {
         "description": "Play/Pause",
         "global": true,
         "suggested_key": {
            "default": "MediaPlayPause"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "agent.js" ],
      "matches": [ "https://*.spotify.com/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "An extension for Spotify shortcuts and controls, based on SpotifyControls by Sergej Popov.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 2,
   "name": "Spotify Controls",
   "permissions": [ "activeTab", "notifications", "storage", "https://*.spotify.com/", "*://*.cloudfront.net/*", "*://*.scdn.co/*", "*://lyrics.wikia.com/*" ],
   "id":"gjdkgeaecjjfgidjgcmkoankdcaljogl",
   "update_url": "http://www.example.com/update.xml",
   "version": "1.0.0"
}
